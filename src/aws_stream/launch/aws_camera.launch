<?xml version="1.0"?>
<!--Open Back Camera With Fisheye Calibration-->

<launch>

	<node name="front_cam" pkg="usb_cam" type="usb_cam_node" respawn="true">
        <!-- <param name="video_device" value="/dev/front_cam" />  -->
        <param name="video_device" value="/dev/video0" /> 
        <param name="image_width" value="640" />
        <param name="image_height" value="480" />
        <param name="image_flip" value="true" />
        <param name="image_flip_code" value="-1" />
        <param name="pixel_format" value="mjpeg" />
        <param name="camera_frame_id" value="usb_cam" />
        <param name="io_method" value="mmap" />
        <param name="framerate" value="20" />
        <param name="autofocus" value="true" />
        <param name="camera_info_url" value="package://usb_cam/config/CameraCalibrationFile.yaml" />
    </node>


	<node name="back_cam" pkg="usb_cam" type="usb_cam_node" respawn="true">
        <!-- <param name="video_device" value="/dev/back_cam" />  -->
        <param name="video_device" value="/dev/video0" /> 
        <param name="image_width" value="640" />
        <param name="image_height" value="480" />
        <param name="pixel_format" value="mjpeg" />
        <param name="camera_frame_id" value="usb_cam" />
        <param name="io_method" value="mmap" />
        <param name="framerate" value="20" />
        <param name="autofocus" value="true" />
        <param name="camera_info_url" value="package://usb_cam/config/CameraCalibrationFile.yaml" />
    </node>
<!-- 
    <node name="image_view" pkg="image_view" type="image_view" respawn="false" output="screen">
        <remap from="image" to="/usb_cam/image_raw"/>
        <param name="autosize" value="true" />
    </node>
     -->
    <!--AWS  kinesisvideo-ros1 -->
    <include file="$(find aws_stream)/launch/front_cam/aws_kinesis.launch" />
    <include file="$(find aws_stream)/launch/back_cam/aws_kinesis.launch" />

    <!-- AWS launch h264_video_encoder -->
    <include file="$(find aws_stream)/launch/front_cam/h264_video_encoder.launch" />
    <include file="$(find aws_stream)/launch/back_cam/h264_video_encoder.launch" />


</launch>
